    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^5.4",
        "illuminate/support": "^5.6"
    },
    "authors": [
        {
            "name": "CKSource",
            "homepage": "https://cksource.com/"
        },
        {
            "name": "AmirArsalan Ebrahimi",
            "email": "me@amirarsalanebrahimi.com",
            "homepage": "http://amirarsalanebrahimi.com"
        }
    ],
    "autoload": {
        "psr-4": {
